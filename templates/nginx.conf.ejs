<% if (config['worker_processes']) { %>
worker_processes  <%- config['worker_processes'] %>;
<% } %>
<% if (config.options) { %>
<% for (key of Object.keys(config.options)) { %>
<%- key %> <%- config.options[key] %>
<% } %>
<% } %>
<% if (config.events) { %>
events {
    <% for (key of Object.keys(config.events)) { %>
    <%- key %> <%- config.events[key] %>;
    <% } %>
}
<% } %>
<% if (config.http) { %>
<% for (httpBlock of config.http) { %>
http {
    <% for (key of Object.keys(httpBlock.options)) { %>
    <%- key %> <%- httpBlock.options[key] %>;
    <% } %>
    <% for (server of httpBlock.servers) { %>
    server {
        <% for (key of Object.keys(server.options)) { %>
        <%- key %> <%- server.options[key] %>;
        <% } %>
        <% for (location of server.locations) { %>
        location <%- location.path %> {
            <% for (key of Object.keys(location.options)) { %>
            <%- key %> <%- location.options[key] %>;
            <% } %>
        }
        <% } %>
    }
    <% } %>
}
<% } %>
<% } %>
